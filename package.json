{
   "name": "adventure-pg-adapter",
   "version": "1.0.0",
   "description": "",
   "main": "lib/src/index.js",
   "types": "lib/src/index.d.ts",
   "scripts": {
      "build": "tsc",
      "format": "prettier --write \"src/**/*.{ts,js}\"",
      "lint": "tslint -p tsconfig.json",
      "migrate:list": "npx ts-node node_modules/.bin/typeorm migration:show",
      "migrate:up": "npx ts-node node_modules/.bin/typeorm migration:run",
      "migrate:down": "npx ts-node node_modules/.bin/typeorm migration:revert",
      "migrate:create": "npx ts-node node_modules/.bin/typeorm migration:create -n",
      "migrate:generate": "npx ts-node node_modules/.bin/typeorm migration:generate -n"
   },
   "repository": {
      "type": "git",
      "url": "git+https://github.com/time4work/adventure-pg-adapter.git"
   },
   "author": "time2funk",
   "license": "ISC",
   "bugs": {
      "url": "https://github.com/time4work/adventure-pg-adapter/issues"
   },
   "homepage": "https://github.com/time4work/adventure-pg-adapter#readme",
   "devDependencies": {
      "@types/node": "^8.0.29",
      "@types/uuid": "^8.3.1",
      "prettier": "^2.4.1",
      "ts-node": "^3.3.0",
      "tslint": "^6.1.3",
      "tslint-config-prettier": "^1.18.0",
      "typescript": "3.3.3333"
   },
   "dependencies": {
      "pg": "^8.4.0",
      "reflect-metadata": "^0.1.10",
      "typeorm": "0.2.38",
      "uuid": "^8.3.2"
   }
}
